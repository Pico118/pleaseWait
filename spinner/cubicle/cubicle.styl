#cubicle
  height 30px
  margin auto
  width 30px

#cubicle div
  animation-direction normal
  animation-duration 1s
  animation-iteration-count infinite
  animation-name cubicle
  animation-play-state inherit
  animation-timing-function linear
  float left
  height 11px
  margin 2px
  outline 0 solid #000
  width 11px

#cubicle div:nth-child(0n+1)
  animation-delay 0s
  background-color fgColor
  outline-color fgColor

#cubicle div:nth-child(0n+2)
  animation-delay 0.25s
  background-color fgColor
  outline-color fgColor

#cubicle div:nth-child(0n+3)
  animation-delay 0.5s
  background-color fgColor
  outline-color fgColor

#cubicle div:nth-child(0n+4)
  animation-delay 0.75s
  background-color fgColor
  outline-color fgColor

@keyframes cubicle
  from
    outline-width 0

  6.25%
    outline-width 2px

  12.5%
    outline-width 0

  to
    outline-width 0
